{
  "name": "openallpdffiles",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "electron .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:mac": "electron-builder --mac",
    "build:win": "electron-builder --win",
    "build:linux": "electron-builder --linux",
    "build:all": "electron-builder --mac --win --linux"
  },
  "build": {
    "appId": "com.matteokoi.openallpdffiles",
    "productName": "OpenAllPDFs",
    "directories": {
      "output": "dist"
    },
    "mac": {
      "category": "public.app-category.utilities",
      "icon": "icon.png",
      "target": [
        "dmg",
        "zip"
      ]
    },
    "win": {
      "icon": "icon.png",
      "target": [
        "nsis",
        "zip"
      ]
    },
    "linux": {
      "icon": "icon.png",
      "target": [
        "AppImage"
      ],
      "category": "Utility"
    }
  },
  "keywords": [],
  "author": "Matteo Koi",
  "license": "ISC",
  "description": "An Electron app to find and open all PDF links from a web page.",
  "devDependencies": {
    "electron": "^40.6.0",
    "electron-builder": "^24.13.3"
  }
}